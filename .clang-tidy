Checks: '
    readability-*, -readability-implicit-bool-conversion,
    clang-analyzer-*, clang-analyzer-cplusplus*,
    modernize-*, -modernize-use-trailing-return-type,
    performance-unnecessary-value-param, -readability-braces-around-statements'

CheckOptions:
  - key:             readability-identifier-naming.ClassCase
    value:           aNy_CasE
  - key:             readability-identifier-naming.ClassMemberCase
    value:           aNy_CasE
  - key:             readability-identifier-naming.ClassMethodCase
    value:           aNy_CasE
  - key:             readability-identifier-naming.StructCase
    value:           aNy_CasE
  - key:             readability-identifier-naming.MemberCase
    value:           aNy_CasE
  - key:             readability-identifier-naming.MethodCase
    value:           aNy_CasE

  - key:             readability-identifier-naming.EnumCase
    value:           aNy_CasE
  - key:             readability-identifier-naming.UnionCase
    value:           aNy_CasE

  - key:             readability-identifier-naming.FunctionCase
    value:           aNy_CasE
  - key:             readability-identifier-naming.ParameterCase
    value:           aNy_CasE
  - key:             readability-identifier-naming.VariableCase
    value:           aNy_CasE

  - key:             readability-identifier-naming.GloabalVariableCase
    value:           UPPER_CASE

  - key:             readability-identifier-naming.NamespaceCase
    value:           aNy_CasE


  - key:             readability-identifier-naming.TemplateParameterCase
    value:           aNy_CasE
  - key:             readability-identifier-naming.TemplateParameterPrefix
    value:           T_